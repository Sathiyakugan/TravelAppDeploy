<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/template/head.ejs %>
</head>
<body class="fixed-nav sticky-footer bg-dark" id="page-top">
<!-- Navigation-->
<% include nav.ejs %>
<div class="content-wrapper">
    <div class="container-fluid">
        <!-- Breadcrumbs-->
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="#">Dashboard</a>
            </li>
            <li class="breadcrumb-item active">My Dashboard/<%= username %></li>
        </ol>

        <%- messages('message', locals) %>
        <!-- Icon Cards-->
        <div class="row">
            <div class="col-xl-3 col-sm-6 mb-3" >
                <div class="card text-white bg-primary o-hidden h-100">
                    <div class="card-body">
                        <div class="card-body-icon">
                            <i class="fa fa-fw fa-circle"></i>
                        </div>
                        <div class="mr-5">Staffs <%=numOfStaffs%></div>
                    </div>
                    <a class="card-footer text-white clearfix small z-1" href="/admin/staffs/view">
                        <span class="float-left">View Details</span>
                        <span class="float-right">
                <i class="fa fa-angle-right"></i>
              </span>
                    </a>
                </div>
            </div>
            <div class="col-xl-3 col-sm-6 mb-3" >
                <div class="card text-white bg-warning o-hidden h-100">
                    <div class="card-body">
                        <div class="card-body-icon">
                            <i class="fa fa-fw fa-user"></i>
                        </div>
                        <div class="mr-5">Users  <%=numOfAppUser%> </div>
                    </div>
                    <a class="card-footer text-white clearfix small z-1" href="/admin/users/view">
                        <span class="float-left">View Details</span>
                        <span class="float-right">
                <i class="fa fa-angle-right"></i>
              </span>
                    </a>
                </div>
            </div>
            <div class="col-xl-3 col-sm-6 mb-3" >
                <div class="card text-white bg-success o-hidden h-100">
                    <div class="card-body">
                        <div class="card-body-icon">
                            <i class="fa fa-fw fa-map"></i>
                        </div>
                        <div class="mr-5">Places <%=numOfAppUser%> </div>
                    </div>
                    <a class="card-footer text-white clearfix small z-1" href="/admin/places/view">
                        <span class="float-left">View Details</span>
                        <span class="float-right">
                <i class="fa fa-angle-right"></i>
              </span>
                    </a>
                </div>
            </div>
            <div class="col-xl-3 col-sm-6 mb-3" >
                <div class="card text-white bg-dark o-hidden h-100">
                    <div class="card-body">
                        <div class="card-body-icon">
                            <i class="fa fa-fw fa-map"></i>
                        </div>
                        <div class="mr-5">Reports </div>
                    </div>
                    <a class="card-footer text-white clearfix small z-1" href="/admin/reports/view">
                        <span class="float-left">View Details</span>
                        <span class="float-right">
                <i class="fa fa-angle-right"></i>
              </span>
                    </a>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xl-12">
                <div class="card mb-3">
                    <div class="card-header">
                        <i class="fa fa-bell-o"></i> Report </div>
                    <div class="list-group list-group-flush small">
                        <a class="list-group-item list-group-item-action" href="#">
                            <div class="media">
                                <img class="d-flex mr-3 rounded-circle" src="http://placehold.it/45x45" alt="">
                                <div class="media-body">
                                    <strong>Sathiyakugan </strong> Added Nallur to the Place
                                    <div class="text-muted smaller">Today at 12:33 PM</div>
                                </div>
                            </div>
                        </a>

                        <a class="list-group-item list-group-item-action" href="#">
                            <div class="media">
                                <img class="d-flex mr-3 rounded-circle" src="http://placehold.it/45x45" alt="">
                                <div class="media-body">
                                    <strong>Sathiyakugan </strong> Logged In to system
                                    <div class="text-muted smaller">Today at 12:40 PM</div>
                                </div>
                            </div>
                        </a>
                        <a class="list-group-item list-group-item-action" href="#">
                            <div class="media">
                                <img class="d-flex mr-3 rounded-circle" src="http://placehold.it/45x45" alt="">
                                <div class="media-body">
                                    <strong>Sathiyakugan </strong> was added to the system
                                    <div class="text-muted smaller">Today at 12:33 PM</div>
                                </div>
                            </div>
                        </a>
                    </div>
                    <div class="card-footer small text-muted">Updated Today</div>
                </div>
            </div>
        </div>
    </div>
        </div>

        <!-- Area Chart Example-->
        <!-- Example DataTables Card-->
    </div>
    <!-- /.content-wrapper-->
    <% include ../partials/template/footer.ejs %>
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fa fa-angle-up"></i>
    </a>
    <!-- Logout Modal-->
    <% include ../partials/template/logout.ejs %>
    <!-- Bootstrap core JavaScript-->
    <% include ../partials/template/scripts.ejs %>
</div>
</body>

</html>